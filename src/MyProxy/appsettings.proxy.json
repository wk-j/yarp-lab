{
    "ReverseProxy": {
        "Routes": {
            "app1": {
                "ClusterId": "app1",
                "Match": {
                    "Path": "app1/{**catch-all}"
                },
                "Transforms": [
                    {
                        "PathRemovePrefix": "/app1"
                    }
                ]
            },
            "app2": {
                "ClusterId": "app2",
                "Match": {
                    "Path": "app2/{**catch-all}"
                },
                "Transforms": [
                    {
                        "PathRemovePrefix": "/app2"
                    }
                ]
            }
        },
        "Clusters": {
            "app1": {
                "Destinations": {
                    "destination1": {
                        "Address": "http://app1:8080"
                    }
                }
            },
            "app2": {
                "Destinations": {
                    "destination1": {
                        "Address": "http://app2:8080"
                    }
                }
            }
        }
    }
}
